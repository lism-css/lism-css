---
import DemoLayout from '@/layouts/DemoLayout.astro';
import { Container } from 'lism-css/astro';
import './_src.css';

// HTMLファイルを文字列としてインポート
import srcHtml from './_src.html?raw';
---

<DemoLayout title="with lism" lism="main">
	<Container isWrapper hasGutter my="40" layout="flow">
		<Fragment set:html={srcHtml} />
	</Container>
</DemoLayout>
